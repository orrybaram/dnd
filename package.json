    "babel-preset-es2015": "^6.3.13",
    "gulp": "latest",
    "gulp-ruby-sass": "latest",
    "gulp-sass": "^2.1.1",
    "ng-annotate-loader": "^0.1.0",
    "ng-annotate-webpack-plugin": "^0.1.2",
